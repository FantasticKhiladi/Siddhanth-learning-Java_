	int n =4;
		int i =1;
		while(i<=n)
		{ int j=1;                                          // 1111
		while(j<=n)                                         //2222  
		{System.out.print(i);                               //3333
		j++;                                                //4444
	}   System.out.println();
		i++;
		}
	}
}
